{
	"name": "ro-component-library",
	"version": "1.10.1",
	"description": "",
	"main": "main.js",
	"types": "./index.d.ts",
	"scripts": {
		"start": "yarn run storybook",
		"test": "jest --watch",
		"test:bail": "jest --watch -b",
		"coverage": "jest --coverage",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"deploy": "yarn run build && yarn publish",
		"build:dev": "sh ./scripts/build-dev.sh",
		"build": "rimraf dist && sh ./scripts/build-dev.sh",
		"postversion": "git push --tags && echo \"Successfully released version $npm_package_version!\""
	},
	"author": "",
	"license": "ISC",
	"peerDependencies": {
		"baseui": ">=5.12.0",
		"react": ">16.5.2",
		"react-dom": ">16.5.2",
		"react-icons": ">3.2.1",
		"react-redux": ">5.0.0",
		"redux": ">4.0.1",
		"styletron-engine-atomic": ">=1.0.13",
		"styletron-react": ">=4.4.4",
		"styletron-react-core": ">=1.3.3",
		"styletron-standard": ">=2.0.1"
	},
	"dependencies": {
		"babel-loader": "^8.0.4",
		"c3": "^0.6.8",
		"d3": "^5.7.0",
		"highcharts": "^6.2.0",
		"highcharts-react-official": "1.4.0",
		"moment": "^2.22.2",
		"polished": "^2.3.0",
		"prop-types": "^15.6.2",
		"react-datepicker": "^1.8.0",
		"react-popper": "^1.3.2",
		"react-sidebar": "^3.0.2",
		"styled-components": "^4.0.3"
	},
	"jest": {
		"setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		],
		"collectCoverageFrom": [
			"<rootDir>/src/**/*.{js,jsx}",
			"!<rootDir>/src/**/**.stories.js",
			"!<rootDir>/src/**/stories.js"
		],
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
			"\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
		}
	}
}
