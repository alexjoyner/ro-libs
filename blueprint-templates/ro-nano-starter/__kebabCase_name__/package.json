		"@types/react-dom": "^16.9.7",
		"babel-preset-nano-react-app": "^0.1.0",
		"@testing-library/jest-dom": "^5.7.0",
		"@testing-library/react": "^10.0.4",
		"parcel": "^1.12.4",
		"@babel/core": "^7.9.6",
		"jest": "^26.0.1",
		"ts-jest": "^25.4.0",
		"typescript": "^3.8.3"
	},
	"babel": {
		"presets": [
			"babel-preset-nano-react-app"
		]
	}
